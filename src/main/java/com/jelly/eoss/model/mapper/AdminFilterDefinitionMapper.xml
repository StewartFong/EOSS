<mapper namespace="AdminFilterDefinition">
	<!-- 生成Insert -->
        	<insert id="Insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.jelly.eoss.model.AdminFilterDefinition">
			insert into admin_filter_definition (
				<trim suffixOverrides=",">
                            <if test="id != null and id != ''">
							id,
                            </if>

                            <if test="rule != null and rule != ''">
							rule,
                            </if>

				</trim>
			) values (
				<trim suffixOverrides=",">
                            <if test="id != null and id != ''">
                                #{id},
                            </if>
                            <if test="rule != null and rule != ''">
                                #{rule},
                            </if>
				</trim>
			)
		</insert>

	<!-- 生成Update -->
    <!-- 注意调用该SQL前必须检查参数的正确性，否则可能会误更新 -->
	<update id="Update" parameterType="com.jelly.eoss.model.AdminFilterDefinition">
		update admin_filter_definition set 
		<trim suffixOverrides=",">
                    <if test="id != null and id != ''">
                    	id = #{id},
                    </if>
                    <if test="rule != null and rule != ''">
                    	rule = #{rule},
                    </if>
		</trim>
        where
                <if test="id != null and id != ''">
				id = #{id}
                </if>
            
	</update>

    <!-- 生成UpdateWithNull -->
    <!-- 注意调用该SQL前必须先根据主键查询出该记录所有列数据，再设置某列为null -->
    <update id="UpdateWithNull" parameterType="com.jelly.eoss.model.AdminFilterDefinition">
        update admin_filter_definition set
        <trim suffixOverrides=",">
			id = #{id},
			rule = #{rule},
        </trim>
        where
		id = #{id}
			
    </update>
	
	<!-- 根据表的主键生成Delete -->
		<delete id="DeleteByPk" parameterType="java.io.Serializable">
			
			delete 
			from admin_filter_definition 
			where 
				id = #{id}
				
		</delete>
	<!-- 注意调用该SQL前必须检查参数的正确性，否则可能会误删除 -->
	<delete id="DeleteByPojo" parameterType="com.jelly.eoss.model.AdminFilterDefinition">
		<!-- 请注意，该表没有主键 -->
		delete
		from admin_filter_definition
		where
        <trim suffixOverrides="and">
                <if test="id != null and id != ''">
					id = #{id} and
                </if>
                <if test="rule != null and rule != ''">
					rule = #{rule} and
                </if>
		</trim>
	</delete>

	<!-- 生成Select通用表头 -->
	<sql id="SelectColumns">
		<trim suffixOverrides=",">
				id as "id",
				rule as "rule",
		</trim>
	</sql>
	
	<!-- 生成Select通用查询条件 -->
	<sql id="SelectWheres">
                <if test="id != null and id != ''">
                    and id = #{id}
                </if>
                <if test="rule != null and rule != ''">
                    and rule = #{rule}
                </if>
	</sql>
	
	<!-- 生成Select -->
	<select id="Select" parameterType="com.jelly.eoss.model.AdminFilterDefinition" resultType="com.jelly.eoss.model.AdminFilterDefinition">
		select
		<include refid="SelectColumns"/>
		from admin_filter_definition
		where 1 = 1
		<include refid="SelectWheres"/>
	</select>

    <!-- 生成SelectCount -->
    <select id="SelectCount" parameterType="com.jelly.eoss.model.AdminFilterDefinition" resultType="Long">
        select count(*) ct
        from admin_filter_definition
        <where>
            <include refid="SelectWheres"/>
        </where>
    </select>

    <!-- 根据表的主键生成SelectByPk，该表只有单列主键 -->
	<select id="SelectByPk" parameterType="java.io.Serializable" resultType="com.jelly.eoss.model.AdminFilterDefinition">
		select
		<include refid="SelectColumns"/>
		from admin_filter_definition
		where
				id = #{id}
			
	</select>
</mapper>